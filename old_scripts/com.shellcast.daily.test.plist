<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>

<!-- define change log event label -->
  <key>Label</key>
  <string>com.shellcast.daily.test.plist</string>

<!-- define script that will run -->
  <key>ProgramArguments</key>
  <array>
    <string>/Users/sheila/Documents/github/shellcast-analysis/shellcast_daily_analysis.sh</string>
  </array>

<!-- priority of scheduler -->
  <key>Nice</key>
  <integer>1</integer>

<!-- runs at 4:00 every day -->
  <key>StartCalendarInterval</key>
  <dict>
    <key>Hour</key>
    <integer>10</integer>
    <key>Minute</key>
    <integer>31</integer>
  </dict>


  <key>RunAtLoad</key>
  <false/>
  <key>KeepAlive</key>
  <false/>

